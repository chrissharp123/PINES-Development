            <menuitem label="&common.textbox.paste;" oncommand="util.clipboard.paste()" />
            <menuitem label="&common.textbox.delete;" oncommand="document.popupNode.value = ''" />
            <menuitem label="&common.textbox.select_all;" oncommand="document.popupNode.select()" />
        </menupopup>
        <messagecatalog id="commonStrings" src='/xul/rel_1_6_1_4/server/locale/<!--#echo var="locale"-->/common.properties'/>
        <messagecatalog id="offlineStrings" src='/xul/rel_1_6_1_4/server/locale/<!--#echo var="locale"-->/offline.properties'/>
        <messagecatalog id="authStrings" src='/xul/rel_1_6_1_4/server/locale/<!--#echo var="locale"-->/auth.properties'/>
        <script>dump('finished openils_util_overlay\n');</script>
    </scripts>

</overlay>

