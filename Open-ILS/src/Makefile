	rm -R $(CGIDIR)
	rm -R $(webdir)
	rm -R $(opacextrasdir)

#perl-install and string-templates-install	
ilscore-install:
	@echo $@
	@echo "Installing Perl modules"
	$(MKDIR_P) $(perldir)
	$(MKDIR_P) $(TEMPLATEDIR)
	cp -r ./perlmods/* $(perldir)
	cp -r ./templates/marc $(TEMPLATEDIR)
	cp -r ./templates/password-reset $(TEMPLATEDIR)
	sed -i 's|SYSCONFDIR|/openils/conf|g' '$(DESTDIR)${exec_prefix}/lib/perl5/OpenILS/WWW/Web.pm'
	sed -i 's|SYSCONFDIR|/openils/conf|g' '$(DESTDIR)${exec_prefix}/lib/perl5/OpenILS/WWW/Method.pm'
	@echo "Installing string templates to $(TEMPLATEDIR)"
	$(MKDIR_P) $(TEMPLATEDIR)
	$(MKDIR_P) $(datadir)/overdue/
	cp -r ./templates/strings $(TEMPLATEDIR)
	sed -i 's|LOCALSTATEDIR|${prefix}/var|g' '$(DESTDIR)/openils/conf/oils_sip.xml.example'
	sed -i 's|SYSCONFDIR|/openils/conf|g' '$(DESTDIR)/openils/conf/oils_sip.xml.example'
	sed -i 's|LOCALSTATEDIR|${prefix}/var|g' '$(DESTDIR)/openils/conf/opensrf_core.xml.example'
	sed -i 's|SYSCONFDIR|/openils/conf|g' '$(DESTDIR)/openils/conf/opensrf_core.xml.example'
	sed -i 's|LOCALSTATEDIR|${prefix}/var|g' '$(DESTDIR)/openils/conf/opensrf.xml.example'
	sed -i 's|SYSCONFDIR|/openils/conf|g' '$(DESTDIR)/openils/conf/opensrf.xml.example'
	sed -i 's|LIBDIR|${exec_prefix}/lib|g' '$(DESTDIR)/openils/conf/opensrf.xml.example'
	sed -i 's|BINDIR|${exec_prefix}/bin|g' '$(DESTDIR)${exec_prefix}/bin/autogen.sh'
	sed -i 's|LOCALSTATEDIR|${prefix}/var|g' '$(DESTDIR)${exec_prefix}/bin/autogen.sh'
	sed -i 's|SYSCONFDIR|/openils/conf|g' '$(DESTDIR)${exec_prefix}/bin/autogen.sh'
	sed -i 's|BINDIR|${exec_prefix}/bin|g' '$(DESTDIR)${exec_prefix}/bin/reshelving_complete.srfsh'
	sed -i 's|BINDIR|${exec_prefix}/bin|g' '$(DESTDIR)${exec_prefix}/bin/juv_to_adult.srfsh'
	sed -i 's|BINDIR|${exec_prefix}/bin|g' '$(DESTDIR)${exec_prefix}/bin/long-overdue-status-update.pl'
	sed -i 's|SYSCONFDIR|/openils/conf|g' '$(DESTDIR)${exec_prefix}/bin/long-overdue-status-update.pl'
	sed -i 's|BINDIR|${exec_prefix}/bin|g' '$(DESTDIR)${exec_prefix}/bin/thaw_expired_frozen_holds.srfsh'
	sed -i 's|BINDIR|${exec_prefix}/bin|g' '$(DESTDIR)${exec_prefix}/bin/offline-blocked-list.pl'

webcore-install:
	$(MKDIR_P) $(DESTDIR)$(WEBDIR)
	$(MKDIR_P) $(DESTDIR)$(WEBDIR)/opac/extras/slimpac/
	$(MKDIR_P) $(DESTDIR)$(WEBDIR)/standalone/
	$(MKDIR_P) $(opacextrasdir)
	$(MKDIR_P) $(DESTDIR)$(reportsdir)
	$(MKDIR_P) $(DESTDIR)$(XSLDIR)
	rm -f $(DESTDIR)$(reportsdir)/fm_IDL.xml
	cp $(DESTDIR)/openils/conf/fm_IDL.xml $(DESTDIR)$(WEBDIR)/reports/
	cp -r ../../Open-ILS/web/. $(DESTDIR)$(WEBDIR)
	cp ../../Open-ILS/xsl/*.xsl $(opacextrasdir)
	cp ../../Open-ILS/xsl/*.xsl $(XSLDIR)
	cp -r $(DESTDIR)$(WEBDIR)/opac/skin/default/* $(DESTDIR)$(WEBDIR)/opac/skin/craftsman/
	cp -r ../../Open-ILS/web/opac/skin/craftsman/* $(DESTDIR)$(WEBDIR)/opac/skin/craftsman/
	for i in mresult.xml rresult.xml rdetail.xml advanced.xml myopac.xml cnbrowse.xml; do \
		cd $(DESTDIR)$(WEBDIR)/opac/skin/default/xml/ && rm -f $$i && $(LN_S) index.xml $$i; \
	done
	for i in mresult.xml rresult.xml rdetail.xml advanced.xml myopac.xml cnbrowse.xml; do \
		 cd $(DESTDIR)$(WEBDIR)/opac/skin/craftsman/xml/ && rm -f $$i && $(LN_S) index.xml $$i; \
	done

offline-install:
	@echo "Installing offline CGIs to $(CGIDIR)/offline";
	$(MKDIR_P) $(TMP)
	$(MKDIR_P) $(DESTDIR)$(CGIDIR)/offline;
	$(MKDIR_P) $(datadir)/offline;
	perl -pe "s{##CONFIG##}{/openils/conf}" < ./offline/offline.pl > $(TMP)/offline.pl;
	cp $(TMP)/offline.pl $(DESTDIR)$(CGIDIR)/offline/
	chmod +x $(DESTDIR)$(CGIDIR)/offline/offline.pl

cgi-bootstrap:
	@echo "Installing cgi's to $(DESTDIR)$(CGIDIR)"
	$(MKDIR_P) $(TMP)/cgi-bin
	$(MKDIR_P) $(DESTDIR)$(CGIDIR)
	for i in ./cgi-bin/*cgi; do perl -pe "s{##CONFIG##}{/openils/conf}" < $$i > $(TMP)/$$i; done
	cp $(TMP)/cgi-bin/*cgi $(DESTDIR)$(CGIDIR)
	cp -r ./cgi-bin/support $(DESTDIR)$(CGIDIR)
	chmod 755 $(DESTDIR)$(CGIDIR)/*cgi

server-xul:
	@echo $@
	$(MKDIR_P) $(DESTDIR)$(WEBDIR)
	$(MKDIR_P) $(DESTDIR)$(WEBDIR)/xul/
	@echo "BUILD_ID = $(STAFF_CLIENT_BUILD_ID)"
	@echo "Copying xul into $(DESTDIR)$(WEBDIR)/xul/$(STAFF_CLIENT_BUILD_ID)"
	$(MKDIR_P) "$(DESTDIR)$(WEBDIR)/xul/$(STAFF_CLIENT_BUILD_ID)"
	cp -R ../../Open-ILS/xul/staff_client/build/server "${DESTDIR}${WEBDIR}/xul/${STAFF_CLIENT_BUILD_ID}/"

reporter-install:
	sed -i 's|SYSCONFDIR|/openils/conf|g' '$(DESTDIR)${exec_prefix}/bin/clark-kent.pl'
	sed -i 's|SYSCONFDIR|/openils/conf|g' '$(DESTDIR)${exec_prefix}/bin/find_orphaned_reports.pl'

install-data-local:
	$(MKDIR_P) $(DESTDIR)$(WEBDIR)/opac/common/js
	for i in $(OPACJS); do \
		cd $(DESTDIR)$(WEBDIR)/opac/common/js && \
		rm -f `basename $$i` && $(LN_S) $$i . ; \
	done
	$(MKDIR_P) $(DESTDIR)$(WEBDIR)/js/dojo/opensrf
	for i in $(JSDOJOSRF); do \
		cd $(DESTDIR)$(WEBDIR)/js/dojo/opensrf && \
		rm -f `basename $$i` && $(LN_S) $$i . ; \
	done
	$(MKDIR_P) $(DESTDIR)$(WEBDIR)/js/dojo
	for i in $(JSDOJO); do \
		cd $(DESTDIR)$(WEBDIR)/js/dojo && \
		rm -f `basename $$i` && $(LN_S) $$i . ; \
	done

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
